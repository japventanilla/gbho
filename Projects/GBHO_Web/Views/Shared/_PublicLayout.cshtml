    @Scripts.Render("~/bundles/public/js")	

    <script type="text/javascript">

        var images = new Array()
        images[0] = "/App_Themes/Public/images/slider_1.png?v=1";
        images[1] = "/App_Themes/Public/images/slider_2.png?v=1";
        images[2] = "/App_Themes/Public/images/slider_3.png?v=1";

        var $img = $(".slider"), i = 0, speed = 300;
        window.setInterval(function () {
            $img.fadeOut(speed, function () {
                $img.attr("src", images[(++i % images.length)]);
                $img.fadeIn(speed);
            });
        }, 3000);

    </script>

