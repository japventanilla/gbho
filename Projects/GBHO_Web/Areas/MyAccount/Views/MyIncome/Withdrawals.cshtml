@{
    ViewBag.Title = "My Income";
    Layout = "~/Areas/MyAccount/Views/Shared/_MyAccountLayout.cshtml";
}


<div class="content mt-3">
    <div class="animated fadeIn">
        <div class="row">

        <div class="col-md-12">
            <div class="card">
                <div class="card-header">
                    <strong class="card-title">My</strong> Withdrawals
                </div>
                <div class="card-body">
            <table id="myList" class="table table-striped table-bordered">
            <thead>
                <tr>
                    <th>Cheque No</th>
                    <th>Amount</th>
                    <th>Status</th>
                    <th>Transaction Date</th>
                </tr>
            </thead>
            <tbody>
               <tr>
                   <td>R112345</td>
                   <td>5,500.00</td>
                   <td>Released</td>
                   <td>01/31/2018</td>
               </tr>
            </tbody>
            </table>
                </div>
            </div>
        </div>


        </div>
    </div>
</div>

@section AdditionalScript{

    @Scripts.Render("~/bundles/myaccount/data-table/js")

    <script type="text/javascript">
        $(document).ready(function () {
            $('#myList').DataTable();
        });
    </script>

}
