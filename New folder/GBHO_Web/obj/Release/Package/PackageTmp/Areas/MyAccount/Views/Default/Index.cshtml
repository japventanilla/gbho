@{    
    ViewBag.Title = "Dashboard";
    Layout = "~/Areas/MyAccount/Views/Shared/_MyAccountLayout.cshtml";
}

<div class="content mt-3">
       

    <div class="col-sm-6 col-lg-6">
        <div class="card text-white bg-flat-color-1">
            <div class="card-body-big">
                <div class="dropdown float-right">
                    <i class="fa fa-th-large"></i>
                </div>
                <h1 class="mb-0">
                    <span class="count">@ViewBag.MemberID</span>
                </h1>
                <h5 class="text-light">Member ID</h5>
            </div>
        </div>
    </div>

    <div class="col-sm-6 col-lg-6">
        <div class="card text-white bg-flat-color-2">
            <div class="card-body-big">
                <div class="dropdown float-right">
                    <i class="fa fa-group"></i>
                </div>
                <h1 class="mb-0">
                    <span class="count">@ViewBag.MyMembers</span>
                </h1>
                <h5 class="text-light">My Members</h5>
            </div>
        </div>
    </div>

    <div class="col-sm-6 col-lg-6">
        <div class="card text-white bg-flat-color-3">
            <div class="card-body-big">
                <div class="dropdown float-right">
                    <i class="fa fa-arrow-circle-o-left"></i>
                </div>
                <h1 class="mb-0">
                    <span class="count">@ViewBag.MyLeft</span>
                </h1>
                <h5 class="text-light">My Left</h5>
            </div>
        </div>
    </div>

    <div class="col-sm-6 col-lg-6">
        <div class="card text-white bg-flat-color-4">
            <div class="card-body-big">
                <div class="dropdown float-right">
                    <i class="fa fa-arrow-circle-o-right"></i>
                </div>
                <h1 class="mb-0">
                    <span class="count">@ViewBag.MyRight</span>
                </h1>
                <h5 class="text-light">My Right</h5>
            </div>
        </div>
    </div>

